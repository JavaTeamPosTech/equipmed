          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "if (jsonData.content && jsonData.content.length > 0) {",
                  "    pm.collectionVariables.set('equipamentoId', jsonData.content[0].id);",
                  "    pm.collectionVariables.set('tagPatrimonio', jsonData.content[0].tagPatrimonio);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/equipamentos?sort=unidadeDeSaude,asc&page=0&size=10",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "equipamentos"
              ],
              "query": [
                {
                  "key": "sort",
                  "value": "unidadeDeSaude,asc"
                },
                {
                  "key": "page",
                  "value": "0"
                },
                {
                  "key": "size",
                  "value": "10"
                },
                {
                  "key": "cidade",
                  "value": "Recife",
                  "disabled": true
